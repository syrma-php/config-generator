{
  "check-requirements": false,
  "dump-autoload": false,
  "stub": "bin/config-generator",
  "compactors": [
    "KevinGH\\Box\\Compactor\\Json",
    "KevinGH\\Box\\Compactor\\Php"
  ],
  "finder": [
    {
      "name": [
        "*.php"
      ],
      "exclude": [
        "Test",
        "test",
        "Tests",
        "tests",
        "doc",
        "docs"
      ],
      "in": [
        "src",
        "vendor"
      ]
    }
  ],
  "main": false,
  "compression": "GZ",
  "output": "build/config-generator.phar",
  "datetime": "release-date",
  "git-version": "git-version"
}
